# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-service-role-key
SUPABASE_SCHEMA=family_memories

# Zadara NGOS
STORAGE_TYPE=zadara
ZADARA_ENDPOINT=https://vsa-XXXXX-public-REGION.zadarazios.com
ZADARA_REGION=us-east-1
ZADARA_ACCESS_KEY_ID=
ZADARA_SECRET_ACCESS_KEY=
ZADARA_BUCKET_NAME=family-memories
ZADARA_PUBLIC_URL=https://vsa-XXXXX-public-REGION.zadarazios.com/family-memories

# Google OAuth (for Google Photos integration)
# Get these from Google Cloud Console: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-client-secret
# Must match exactly in Google OAuth consent screen authorized redirect URIs
# For Cloud Run: https://your-app-xxx.region.run.app/api/auth/google/callback
# For local dev: http://localhost:3000/api/auth/google/callback
GOOGLE_REDIRECT_URI=http://localhost:3000/api/auth/google/callback
# Random 32+ character string for encrypting OAuth tokens
TOKEN_ENCRYPTION_KEY=your-random-32-character-secret-key

# App
NEXT_PUBLIC_APP_NAME=Family Memories
